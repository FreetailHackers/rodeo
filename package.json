		"@sveltejs/vite-plugin-svelte": "^4.0.4",
		"@types/crypto-js": "^4.1.1",
		"@types/file-saver": "^2.0.5",
		"@types/node": "^18.11.18",
		"@types/nodemailer": "^6.4.7",
		"@types/qrcode": "^1.5.0",
		"@types/stopword": "^2.0.3",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^3.8.2",
		"pre-commit": "^1.2.2",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.4.0",
		"prisma": "^5.10.1",
		"sv-popup": "^0.5.3",
		"svelte": "^5.33.13",
		"svelte-check": "^4.2.1",
		"ts-node": "^10.9.1",
		"tslib": "^2.4.1",
		"tsx": "^4.19.4",
		"typescript": "^5.5.0",
		"vite": "^5.4.4",
		"vitest": "^3.2.1",
		"vitest-mock-extended": "^1.1.3"
	},
	"type": "module",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.414.0",
		"@aws-sdk/s3-request-presigner": "^3.388.0",
		"@humanspeak/svelte-markdown": "^0.8.7",
		"@json2csv/plainjs": "^7.0.1",
		"@mantine/core": "^8.1.0",
		"@mantine/hooks": "^8.1.0",
		"@node-rs/argon2": "^2.0.2",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@pilcrowjs/object-parser": "^0.0.4",
		"@prisma/client": "^5.10.1",
		"@trpc/client": "^10.7.0",
		"@trpc/server": "^10.7.0",
		"@types/marked": "^5.0.2",
		"arctic": "^3.7.0",
		"argon2": "^0.43.0",
		"crypto-js": "^4.1.1",
		"dayjs": "^1.11.7",
